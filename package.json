{
    "author": "",
    "bugs": {
        "url": "https://github.com/vhs/tacos/issues"
    },
    "dependencies": {},
    "description": "TACOS is a React/NodeJS based solution for managing tool access control in a hack-/makerspace environment.",
    "devDependencies": {
        "concurrently": "^6.3.0"
    },
    "homepage": "https://github.com/vhs/tacos#readme",
    "keywords": [],
    "license": "ISC",
    "name": "tacos",
    "packageManager": "pnpm@9.15.1+sha512.1acb565e6193efbebda772702950469150cf12bcc764262e7587e71d19dc98a423dff9536e57ea44c49bdf790ff694e83c27be5faa23d67e0c033b583be4bfcf",
    "prettier": "@tyisi/config-prettier",
    "private": true,
    "repository": {
        "type": "git",
        "url": "git+https://github.com/vhs/tacos.git"
    },
    "resolutions": {
        "levelup-defaults": "^2.0.0"
    },
    "scripts": {
        "build": "pnpm --filter=tacos-client run build",
        "client": "pnpm --filter=tacos-client start",
        "dev": "concurrently --kill-others-on-fail \"pnpm run server-dev\"  \"pnpm run client\"",
        "postbuild": "rsync -avz ./client/build/ ./server/public/",
        "prebuild": "echo Starting build...",
        "server": "pnpm --filter=tacos-server start",
        "server-dev": "pnpm --filter=tacos-server run dev",
        "start": "concurrently --kill-others-on-fail \"pnpm run server\"  \"pnpm run client\"",
        "test": "echo \"Error: no test specified\" && exit 1"
    },
    "version": "1.0.0"
}
